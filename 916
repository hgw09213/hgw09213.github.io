public class Second {
	public static void main(String[] args) {
		Account myObj = new Account("honggyeongwoo", "dongseo", "01049302559");
		Account myObj1 = new Account("parksoon", "computer", "01012345678");
		System.out.println(myObj.name);
		System.out.println(myObj.address);
		System.out.println(myObj.balance);
	}
}

